#include<iostream>
using namespace std;
int main()
{
    int a,b,c,d,e,f,g;
    cin>>a>>b;
    c=a+b;
    d=a-b;
    e=a*b;
    f=a/b;
    g=a%b;
    cout<<a<<"+"<<b<<"="<<c<<endl;
    cout<<a<<"-"<<b<<"="<<d<<endl;
    cout<<a<<"*"<<b<<"="<<e<<endl;
    cout<<a<<"/"<<b<<"="<<f<<endl;
    cout<<a<<"%"<<b<<"="<<g<<endl;
    
    
}
