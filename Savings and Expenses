#include<iostream>
using namespace std;
int main()
{
    int a,b,c,d,e;
    cin>>a>>b>>c>>d>>e;
    if(a<(b+c+d+e))
    {
        cout<<"He wants to work hard";
    }
    else if(a>(b+c+d+e))
    {
        cout<<"He can save the money";
    }
    else 
    {
        cout<<"His salary will be able to manage his expenses alone";
    }
}
